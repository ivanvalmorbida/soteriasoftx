md-dialog(aria-label="Localizar Usuario", flex='80')
  form(ng-cloak="")
    md-toolbar
      .md-toolbar-tools
        h2 Localizar usuario
        span(flex="")
        md-button.md-icon-button(ng-click="Cancel()")
          md-icon(md-svg-src="img/fechar.svg", aria-label="Fechar di√°logo")
    md-dialog-content
      .md-dialog-content
        div(layout='row')
          md-input-container.md-block(style='margin-top: 10px;')
            label campo
            md-select(ng-model='campopesq')
              md-option(value='usuario') Usuario
              md-option(value='pessoa') Pessoa
            
          md-input-container.md-block(flex='',style='margin-top: 10px;')
            label localizar
            input(ng-model='txtpesquisa')

          #btLocalizarExe.md-button.md-raised.md-primary.md-hue-3(
            ng-click='LocalizarExe(campopesq, txtpesquisa)',
            ng-disabled='txtpesquisa==undefined') Executar

        div(layout='row')
          div(flex="") 
            | Usuario   
          div(flex="") Pessoa
          |   
          div(flex="") Tipo
          |   

        div(layout='row', ng-repeat="d in l_dados")
          div(flex="") 
            a(ng-click='ExibirUsuario(d.codigo)') {{d.usuario}}   
          div(flex="") {{d.pessoa}}
          |   
          div(flex="") {{d.tipo}}
          |   
              
    md-dialog-actions(layout="row")
      span(flex="")
      md-button.md-raised(type='button', ng-click="Cancel()") Fechar